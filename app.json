{
  "expo": {
    "name": "meteo-companion",
    "slug": "meteo-companion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a2b3e5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.meteo.companion",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#a2b3e5"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6a4e79cd-608c-4f75-a34a-d7b106782bdb"
      }
    },
    "owner": "jesuisstan"
  }
}
